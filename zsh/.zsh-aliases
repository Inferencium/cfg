# Navigation.
alias ..='cd ..'
alias cl='clear'
alias l='ls -CF --color=auto'
alias la='ls -A --color=auto'
alias ll='ls -l --color=auto'
alias ls='ls -F --color=auto'
alias tr='tree -C'

# File management.
alias clp='wl-copy'
alias cp='cp -iv'
alias md='mkdir -v'
alias mv='mv -iv'
alias rm='rm -iv'
